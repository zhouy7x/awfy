[main]
repos = /home/user/work/repos
benchmarks = /home/user/work/awfy/benchmarks
cpu = arm
machine = 0
updateURL = http://ssgs5-test.sh.intel.com/UPDATE.php
flags = hardfp
slaves = chromeos
modes = v8-turbofan-arm

[native]
options = -O2
cc = gcc
cxx = g++
mode = gcc

[v8]
source = v8

[v8-default-arm]
arg1 = --no-turbo

[v8-turbofan-arm]

[chromeos]
machine = 6
hostname = root@awfy-chromebook-turbofan.sh.intel.com
repos = /usr/local/awfy/repos
benchmarks = /usr/local/awfy/benchmarks
driver = /usr/local/awfy/driver
python = python
includes = octane,WebTooling,ARES6,Jetstream2D8,d8,snapshot_blob,polybench,spec2k6
