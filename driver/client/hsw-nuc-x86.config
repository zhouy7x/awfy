[main]
repos = /home/user/work/repos
benchmarks = /home/user/work/awfy/benchmarks
cpu = x86
machine = 0
updateURL = http://ssgs3-test.sh.intel.com/UPDATE.php
slaves = hsw
modes = v8-turbofan-x86

[native]
options = -O2
cc = gcc
cxx = g++
mode = gcc

[v8]
source = v8

[v8-default-x86]
arg1 = --no-turbo

[v8-turbofan-x86]

[v8-optisize-x86]
arg1 = --optimize-for-size

[hsw]
machine = 1
hostname = user@hsw-nuc.sh.intel.com
repos = /home/user/work/repos
benchmarks = /home/user/work/awfy/benchmarks
driver = /home/user/work/awfy/driver
python = python
includes = octane,WebTooling,ARES6,jetstream2_d8
