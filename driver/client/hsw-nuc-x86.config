[main]
repos = /home/user/work/awfy/repos
benchmarks = /home/user/work/awfy/benchmarks
cpu = x86
machine = 0
updateURL = http://user-awfy.sh.intel.com/awfy/UPDATE.php
slaves = hsw
modes = v8-default-x86,v8-turbofan-x86,v8-optisize-x86

[native]
options = -O2
cc = gcc
cxx = g++
mode = gcc

[v8]
source = v8

[v8-default-x86]

[v8-turbofan-x86]
arg1 = --turbo

[v8-optisize-x86]
arg1 = --optimize-for-size

[hsw]
machine = 1
hostname = user@hsw-nuc.sh.intel.com
repos = /home/user/work/awfy/repos
benchmarks = /home/user/work/awfy/benchmarks
driver = /home/user/work/awfy/driver
python = python

